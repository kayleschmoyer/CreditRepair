{
  "name": "creditcraft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:reset": "psql $SUPABASE_DB_URL -c 'DROP SCHEMA public CASCADE; CREATE SCHEMA public;'",
    "db:apply": "psql $SUPABASE_DB_URL -f sql/migration.sql",
    "db:seed": "ts-node sql/seed.ts",
    "db:reseed": "npm run db:reset && npm run db:apply && npm run db:seed",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.0",
    "@supabase/auth-helpers-nextjs": "0.10.0",
    "@supabase/auth-ui-react": "0.2.7",
    "@supabase/auth-ui-shared": "0.1.8",
    "@supabase/supabase-js": "2.39.8",
    "jszip": "^3.10.1",
    "lob": "^6.6.3",
    "next": "14.0.0",
    "nodemailer": "6.9.5",
    "pdf-lib": "1.17.1",
    "qrcode": "^1.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.1",
    "resend": "6.0.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "1.38.0",
    "@types/node": "20.8.10",
    "@types/nodemailer": "7.0.1",
    "@types/qrcode": "1.5.5",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "eslint": "^8.47.0",
    "eslint-config-next": "14.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  }
}
