    "jszip": "^3.10.1",
    "lob": "^6.6.3",
    "next": "14.0.0",
    "nodemailer": "6.9.5",
    "pdf-lib": "1.17.1",
    "qrcode": "^1.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
